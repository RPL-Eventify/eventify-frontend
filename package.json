    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "postcss": "^8",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.3.0"
